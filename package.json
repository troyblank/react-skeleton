{
  "name": "react-skeleton",
  "version": "0.1.0",
  "main": "./dist/index.html",
  "scripts": {
    "start": "yarn clean && yarn watch",
    "clean": "rm -rf dist && rm -rf coverage",
    "watch": "vite",
    "build": "vite build",
    "test": "yarn lint && yarn unit:once",
    "test:ci": "yarn tsc:react && jest --maxWorkers=50% --ci --config ./jest.config.js",
    "lint": "yarn lint:react",
    "lint:react": "yarn tsc:react && eslint --max-warnings=0 src --ext .js,.ts,.tsx",
    "tsc:react": "tsc --project tsconfig.json --noEmit",
    "unit": "jest --watch --config ./jest.config.js",
    "unit:once": "jest --config ./jest.config.js --maxWorkers=25%"
  },
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "@troyblank/eslint-config-troyblank": "2.1.0",
    "@testing-library/dom": "9.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/chance": "1.1.3",
    "@types/jest": "29.5.1",
    "chance": "1.1.11",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "ts-jest": "29.1.0"
  },
  "dependencies": {
    "@types/lodash": "4.14.194",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@types/react-query": "1.2.8",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "eslint": "8.39.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.10.3",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "4.0.0",
    "react-router-dom": "6.10.0",
    "source-map-support": "0.5.21",
    "styled-components": "5.3.10",
    "typescript": "5.0.4",
    "vite": "4.3.1"
  }
